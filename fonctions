chalnge 3 


#include<stdio.h>

int max(int num1,int num2){
       if (num1<num2){
        printf("num2 is big than num1");
       }else if(num1=num2){
        printf("num1 egal num2");
       }else{
        printf("num1 is big than num2");
       }
}
int main(){
    int num1=2, num2=8;

    printf( "",max(num1, num2));


}

chalenge 4:



#include<stdio.h>

int max(int num1,int num2){
       if (num1<num2){
        printf("num2 is smaller than num1");
       }else if(num1=num2){
        printf("num1 egal num2");
       }else{
        printf("num1 is smaller than num2");
       }
}
int main(){
    int num1=2, num2=8;

    printf( "",max(num1, num2));


}

chalange 5:

#include<stdio.h>

int fact(int num){
    int mult=1;

    for(int i=1;i<=num;i++){
        mult=mult*i;
    
    }
    return mult;
     
    }  

int main(){
    int num;

    printf("Pleas enter a number : ");
    scanf("%d",&num);
    printf("multiplication de %d = %d",num,fact(num));

}
