#include <stdio.h>

struct personne {
    char nom[100];
    char prenom[100];
    int age;
};

int main(){
    struct personne p = {"Khalid","Bensaada",18};

    printf("nom    : %s",p.nom);
    printf("nom    : %s",p.prenom);
    printf("nom    : %d",p.age);
}


Challenge 2:

#include <stdio.h>

struct etudiant {
    char nom[100];
    char prenom[100];
    int note[6];
};

int main(){
    struct etudiant e ;
    printf("entrez le nom etudiant : ");
    scanf("%s",e.nom);
    printf("entrez le prenom etudiant : ");
    scanf("%s",e.prenom);
    for (int i =0; i<6; i++) {
        printf("note %d : ",i);
        scanf("%d",&e.note[i]);

    
    }

    printf("nom       : %s \n",e.nom);
    printf("prenom    : %s \n",e.prenom);
    for (int i = 0; i<6 ; i++) {
        printf("note %d   : %d \n",i,e.note[i]);
    
    }
}

chalange 3

#include<stdio.h>
struct rectangle{
     float longueur ;
     float  largeur;
};

int main(){
    struct rectangle n ;
    float aire;

    printf("Plaese enter longueur: ");
    scanf("%f",&n.longueur);

    printf("Plaese enter largeur:");
    scanf("%f",&n.largeur);

    aire = n.longueur * n.largeur;
    printf("the total is : %.2f",aire);
}
