EXERCISES 2

#include <stdio.h>

int main() {
    char c;
    printf("Entrez un caractere: ");
    scanf(" %c", &c);

    switch(c) {
        case 'a': case 'e': case 'i': case 'o': case 'u':
        case 'A': case 'E': case 'I': case 'O': case 'U':
            printf("%c est une voyelle.\n", c);
            break;
        default:
            printf("%c n'est pas une voyelle.\n", c);
    }

    return 0;
}



EXERCISES 3


#include <stdio.h>

int main() {
    int a, b;
    printf("Entrez deux nombres: ");
    scanf("%d %d", &a, &b);

    if (a == b)
        printf("Resultat: %d\n", 3 * (a + b));
    else
        printf("Resultat: %d\n", a + b);

    return 0;
}


EXERCISES 4



#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c, delta, x1, x2;
    printf("Entrez les coefficients a, b et c: ");
    scanf("%lf %lf %lf", &a, &b, &c);

    delta = b*b - 4*a*c;

    if (delta > 0) {
        x1 = (-b + sqrt(delta)) / (2*a);
        x2 = (-b - sqrt(delta)) / (2*a);
        printf("Deux solutions reelles: x1=%.2lf, x2=%.2lf\n", x1, x2);
    } else if (delta == 0) {
        x1 = -b / (2*a);
        printf("Une solution reelle: x=%.2lf\n", x1);
    } else {
        printf("Pas de solution reelle.\n");
    }

    return 0;
}


EXERCISES 5 



#include <stdio.h>

int main() {
    int annee;
    printf("Entrez l'annee: ");
    scanf("%d", &annee);

    int mois = annee * 12;
    int jours = annee * 365;
    int heures = jours * 24;
    int minutes = heures * 60;
    int secondes = minutes * 60;

    printf("%d annee(s) = %d mois\n", annee, mois);
    printf("%d jours\n", jours);
    printf("%d heures\n", heures);
    printf("%d minutes\n", minutes);
    printf("%d secondes\n", secondes);

    return 0;
}


