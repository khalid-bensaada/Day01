EXERCISES 5 



#include <stdio.h>

int main() {
    float temp;

    printf("Entrez la température en Celsius : ");
    scanf("%f", &temp);

    if (temp < 0)
        printf("État de l'eau : Solide\n");
    else if (temp >= 0 && temp < 100)
        printf("État de l'eau : Liquide\n");
    else
        printf("État de l'eau : Gaz\n");

    return 0;
}



