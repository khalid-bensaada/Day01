EXERCISE 9


#include <stdio.h>

int main() {
    int n;
    int count = 0;

    printf("Entrez un entier positif : ");
    scanf("%d", &n);

    if (n == 0) {
        count = 1;   
    } else {
        while (n > 0) {
            n = n / 10; 
            count++;     
        }
    }

    printf("Nombre de chiffres = %d\n", count);

    return 0;
}




EXERCISE 7




#include <stdio.h>

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;

    if (n < 0) {
        putchar('-'); 
        n = -n;
    }


    if (n == 0) {
        putchar('0');
        return 0;
    }

    while (n > 0) {
        int d = n % 10;   
        printf("%d", d);  
        n /= 10;         
    }

    return 0;
}




