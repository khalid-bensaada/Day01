LES CONDITIONS 2

EXERCISES 1


#include <stdio.h>

int main() {
    double revenu;
    int score, duree;

    printf("Entrez le revenu annuel (€): ");
    scanf("%lf", &revenu);
    printf("Entrez le score de credit (0-1000): ");
    scanf("%d", &score);
    printf("Entrez la duree du pret (annees): ");
    scanf("%d", &duree);

    if (revenu >= 30000 && score >= 700 && duree <= 10)
        printf("Eligibilite: ELEGIBLE\n");
    else if (revenu >= 30000 && score >= 650 && duree <= 15)
        printf("Eligibilite: ELEGIBLE AVEC CONDITIONS\n");
    else
        printf("Eligibilite: NON ELEGIBLE\n");

    return 0;
}


EXERCISES 2

#include <stdio.h>

int main() {
    int age, typeVoiture, nbAccidents;
    double primeBase = 100, prime; // Exemple de prime de base

    printf("Entrez l'age du conducteur: ");
    scanf("%d", &age);
    printf("Entrez le type de voiture (1=sportive, 2=utilitaire, 3=familiale): ");
    scanf("%d", &typeVoiture);
    printf("Entrez le nombre d'accidents (5 dernieres annees): ");
    scanf("%d", &nbAccidents);

    // Calcul selon age
    if (age < 25) prime = primeBase * 1.5;
    else if (age <= 65) prime = primeBase;
    else prime = primeBase * 1.2;

    // Type de voiture
    if (typeVoiture == 1) prime *= 2;
    else if (typeVoiture == 2) prime *= 1.2;
    else if (typeVoiture == 3) prime *= 1.1;

    // Accidents
    if (nbAccidents > 1) prime *= 1.3;

    printf("Prime d'assurance: %.2lf €\n", prime);

    return 0;
}


EXERCISES 4

#include <stdio.h>

int main() {
    int score, anciennete, recompenses;
    double bonus = 0;

    printf("Entrez le score (0-100): ");
    scanf("%d", &score);
    printf("Entrez l'anciennete (annees): ");
    scanf("%d", &anciennete);
    printf("Entrez les recompenses (0,1,2+): ");
    scanf("%d", &recompenses);

    if (recompenses == 1) bonus = 0.1;
    else if (recompenses >= 2) bonus = 0.2;

    if (score >= 90 && anciennete >= 5)
        printf("Evaluation: Excellente + bonus %.0f%%\n", bonus*100);
    else if (score >= 75 && anciennete >= 3)
        printf("Evaluation: Bonne + bonus %.0f%%\n", bonus*100);
    else if (score >= 50 && anciennete < 3)
        printf("Evaluation: Satisfaisante + bonus %.0f%%\n", bonus*100);
    else
        printf("Evaluation: Insuffisante\n");

    return 0;
}


EXERCISES 5

#include <stdio.h>

int main() {
    double budget;
    int destination, nbPersonnes;

    printf("Entrez le budget (€): ");
    scanf("%lf", &budget);
    printf("Entrez la destination (1=plage, 2=montagne, 3=ville): ");
    scanf("%d", &destination);
    printf("Entrez le nombre de personnes: ");
    scanf("%d", &nbPersonnes);

    if (budget >= 1000) printf("Voyage haut de gamme\n");
    else if (budget >= 500) printf("Voyage moyen\n");
    else printf("Voyage economique\n");

    if (budget >= 1000 && nbPersonnes > 2) printf("Destination recommande: Plage\n");
    else if (budget >= 500 && nbPersonnes <= 2) printf("Destination recommande: Montagne\n");
    else printf("Destination recommande: Ville\n");

    return 0;
}

EXERCISES 8

#include <stdio.h>

int main() {
    int age, historique, couverture;

    printf("Entrez l'age: ");
    scanf("%d", &age);
    printf("Historique medical (0=aucun,1=mineur,2=majeur): ");
    scanf("%d", &historique);
    printf("Type de couverture (1=base,2=etendue): ");
    scanf("%d", &couverture);

    if (age < 30) printf("Plan: Base\n");
    else if (historique == 0) printf("Plan: Base\n");
    else printf("Plan: Etendu\n");

    if (historique == 2) printf("Couverture supplementaire pour problemes majeurs\n");

    return 0;
}



LES TABLEAUX 
EX 1

#include<stdio.h>

int main(){
    int i ,n;
    printf("entrer la taille de tablaux:\n");
    scanf("%d",&n);
    int tab[n];
    
    for(i=0;i<n;i++){
        printf("les element de tablau %d : ",i);
        scanf("%d",&tab[i]);
    }
    for(i=0;i<n;i++){
        printf("tab[%d]= %d \n",i,tab[i]);     
    }
}
