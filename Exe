// Online C compiler to run C program online
#include <stdio.h>

int main() {
    char name[15];
    char namef[15];
    int age ;
    char sexe[15];
    char mail[50];
    printf("Enter your name: ");
    scanf("%s",name);
    printf("Enter your name family: ");
    scanf("%s",namef);
    printf("Enter your age: ");
    scanf("%d",&age);
    printf("Enter your sexe(male or femmel): ");
    scanf("%s",sexe);
    printf("Enter your mail: ");
    scanf("%s",mail);

    return 0;
}

EXERCICE 6


// Online C compiler to run C program online
#include <stdio.h>

int main() {
float num1;
float num2;
char type;
float result;


printf("Please enter first number: ");
scanf("%f",&num1);
printf("Please enter type (+ - * /): ");
scanf("%s",&type);
printf("Please enter second number: ");
scanf("%f",&num2);
switch(type){
    case '+':
         result = num1 +num2;
         printf(" %.2f + %.2f = %.2f",num1,num2,result);
        
         break;
    case '-':
         result = num1-num2;
         printf(" %.2f - %.2f = %.2f",num1,num2,result);
         
         break;
    case '*':
         result = num1*num2;
         printf(" %.2f * %.2f = %.2f",num1,num2,result);
         
         break;
    case '/':
         result = num1/num2;
         printf("%.2f / %.2f = %.2f",num1,num2,result);
         
         break;
    default:
         printf("invalid type");
    
}


    return 0;
}


