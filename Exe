EXERCISES 5




#include <stdio.h>

int main() {
    int base,exposant;
    int result =1;
    
    printf("Enter number base: ");
    scanf("%d",&base);
    printf("Enter number exposant: ");
    scanf("%d",&exposant);
    
    for(int i=0; i<exposant;i++){
        result = result * base;
    }
    printf("result of %d ^ %d = %d",base,exposant, result);
    
    
    

    return 0;
}


LES VARIABLE EX2


#include <stdio.h>

int main() {
    float celsius, kelvin;
    
    printf("Entrez la température en Celsius : ");
    scanf("%f", &celsius);
    
    kelvin = celsius + 273.15;
    
    printf("La température en Kelvin est : %.2f\n", kelvin);
    
    return 0;
}


